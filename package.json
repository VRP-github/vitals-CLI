{
  "name": "vitals-cli",
  "version": "1.0.2",
  "description": "The Terminal Health Monitor. Visualize system vitals, latency, and logs in real-time.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "vitals": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts --clean",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vitals",
    "cli",
    "monitoring",
    "health-check",
    "visualization",
    "devops"
  ],
  "author": "Viraj Patel",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "tsup": "^8.5.1",
    "typescript": "^5.0.0",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "vitals-cli": "^1.0.2"
  }
}
